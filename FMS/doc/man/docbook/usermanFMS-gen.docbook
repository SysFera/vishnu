            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>UserException(&quot;an option or a parameter provided is invalid for this service.&quot; [10])</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>UserException(&quot;Undefined configuration parameter&quot; [12])</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>UMSVishnuException(&quot;The user is not an administrator&quot; [25])</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>UMSVishnuException(&quot;The session key is unrecognized&quot; [28])</term>
            <listitem/>
