      <refsect1 id="description">
        <title>DESCRIPTION</title>
        <para>This command allows getting information about a specific job. It can return the job's status, for example.</para>
      </refsect1>
      <refsect1 id="arguments">
        <title>ARGUMENTS</title>
        <variablelist>
          <varlistentry>
            <term><emphasis>sessionKey</emphasis></term>
            <listitem>
